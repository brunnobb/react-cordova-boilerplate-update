  "scripts": {
    "webpack": "webpack-cli ",
    "analyze": "set NODE_ENV=production&&set ANALYZE=true&&npm run webpack",
    "compile": "set NODE_ENV=development&&npm run webpack",
    "compile:prod": "set NODE_ENV=production&&npm run webpack",
    "browser": "set NODE_ENV=development&&npm run webpack&&http-server ./www/ -i --cors -c-1 -o",
    "browser:prod": "set NODE_ENV=development&&npm run webpack&&http-server ./www/ -i --cors -c-1 -o",
    "hmr": "set NODE_ENV=development&&webpack-dev-server --inline --watch --hot --content-base www/",
    "cbrowser": "set NODE_ENV=development&&npm run webpack&&cordova run browser",
    "android": "set NODE_ENV=development&&npm run webpack&&cordova run android",
    "ios": "set NODE_ENV=development&&npm run webpack&&cordova run ios",
    "windows": "set NODE_ENV=development&&npm run webpack&&cordova run windows",
    "electron": "electron ./electron/main ",
    "build:android": "set NODE_ENV=production&&npm run webpack&&cordova build android",
    "build:ios": "set NODE_ENV=production&&npm run webpack&&cordova build ios",
    "build:windows": "set NODE_ENV=production&&npm run webpack&&cordova build windows",
    "build:electron": "set NODE_ENV=production&&npm run webpack&&electron ./electron/main"
  },
  "cordova": {
    "platforms": [
      "android",
      "windows",
      "browser"
    ]
  }
