{
	"name": "rect-redux-cordova-electron-boilerplate",
	"version": "1.0.0",
	"description": "React-redux Cordova Electron Boilerplate",
	"main": "./main.jsx",
	"author": "Brunno Bettini",
	"license": "ISC",
	"scripts": {
		"webpack": "webpack-cli -d --mode development ",
		"analyze": "cross-env NODE_ENV=production ANALYZE=true&&npm run webpack",
		"compile": "cross-env NODE_ENV=development npm run webpack",
		"compile:prod": "cross-env NODE_ENV=production npm run webpack",
		"browser": "cross-env NODE_ENV=development npm run webpack&&http-server ./www/ -i --cors -c-1 -o",
		"browser:prod": "cross-env NODE_ENV=development npm run webpack&&http-server ./www/ -i --cors -c-1 -o",
		"hmr": "cross-env PORT=8085 HOST=0.0.0.0 NODE_ENV=development webpack-dev-server",
		"cordova-browser": "cross-env NODE_ENV=development npm run webpack&&cordova run browser",
		"android": "cross-env NODE_ENV=development npm run webpack&&cordova run android --device",
		"ios": "cross-env NODE_ENV=development npm run webpack&&cordova run ios --device",
		"build:android": "cross-env NODE_ENV=production npm run webpack&&cordova build android --release",
		"build:ios": "cross-env NODE_ENV=production npm run webpack&&cordova build ios --release",
		"windows": "set NODE_ENV=development&&npm run webpack&&cordova run windows",
		"build:windows": "cross-env NODE_ENV=production npm run webpack&&cordova build windows --release",
		"electron": "electron ./electron/main ",
		"build:electron": "set NODE_ENV=production&&npm run webpack&&electron ./electron/main",
		"cordova:prepare": "cordova prepare",
		"watchbuild": "watchbuild -a com.package.boilerplate -u brunnobb@gmail.com",
		"resources:android": "ionic cordova resources android -f",
		"resources:ios": "ionic cordova resources ios -f",
		"add:ios": "cordova platform add ios",
		"remove:ios": "cordova platform remove ios",
		"remove:windows": "cordova platform remove windows",
		"prettier": "npm run prettier:js&&npm run prettier:jsx",
		"prettier:js": "prettier --write \"**/*.js\"",
		"prettier:jsx": "prettier --write \"**/*.jsx\"",
		"prettier:ts": "prettier --write \"**/*.ts\"",
		"prettier:tsx": "prettier --write \"**/*.tsx\""
	},
